{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "modules": true
        },
        "ecmaVersion": "12",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "prettier/prettier": "warn",
        "no-unused-vars": "off",
        "react/prop-types": "off"
    }
}
